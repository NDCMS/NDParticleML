universe = vanilla
executable = 2d_profiling.sh

Log    = ./logs/$(Cluster)_$(Process).log
Output = ./logs/$(Cluster)_$(Process).out
Error  = ./logs/$(Cluster)_$(Process).err

should_transfer_files = YES
transfer_input_files = 2d_profiling.py, NN_Module_Poly.py, polynomial.py, 6755_13_model+.pt, likelihood_cpQ3_cbW.npz, likelihood_cpQM_ctG.npz, likelihood_cpt_ctp.npz, likelihood_cptb_cQl3i.npz, likelihood_cQei_cQlMi.npz, likelihood_ctei_ctlTi.npz, likelihood_ctli_ctlSi.npz, likelihood_ctW_ctZ.npz, likelihood_profiled_cpQ3_cbW.npz, likelihood_profiled_cpQM_ctG.npz, likelihood_profiled_cpt_ctp.npz, likelihood_profiled_cptb_cQl3i.npz, likelihood_profiled_cQei_cQlMi.npz, likelihood_profiled_ctei_ctlTi.npz, likelihood_profiled_ctli_ctlSi.npz, likelihood_profiled_ctW_ctZ.npz
when_to_transfer_output = ON_EXIT
# Must specify to transfer entire directories
transfer_output_files = graphs

# Enable Singularity feature
## Notre Dame Images
# doesn't give latest Pytorch: +SingularityImage = "/cvmfs/singularity.opensciencegrid.org/notredamedulac/el7-pytorch-gpu:latest"
+SingularityImage = "/afs/crc.nd.edu/x86_64_linux/s/singularity/images/pytorch-1.9.sif"

request_gpus   = 1
request_memory = 10 Gb
request_cpus   = 1

# Don't forget to include training files in transfer_input_files above
queue 1